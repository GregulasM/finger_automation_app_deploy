    "@nuxt/hints": "1.0.0-alpha.5",
    "@nuxt/image": "2.0.0",
    "@nuxt/test-utils": "3.23.0",
    "@nuxt/ui": "^4.3.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@upstash/qstash": "^2.6.0",
    "@upstash/redis": "^1.34.0",
    "@vue-flow/core": "^1.41.1",
