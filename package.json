    "imapflow": "^1.2.6",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.12",
    "nuxt": "^4.2.2",
    "nuxt-auth-utils": "^0.5.27",
    "pg": "^8.16.3",
    "pinia": "^3.0.4",
    "prisma": "^7.2.0",
    "tailwindcss": "^4.1.18",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.12.1",
    "@pinia/nuxt": "^0.11.3",
    "@types/nodemailer": "^7.0.5",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
