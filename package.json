{
  "name": "Finger_automation_app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bunx prisma migrate deploy && bunx nuxt build && bun scripts/copy-prisma.mjs",
    "dev": "bunx nuxt dev",
    "preview": "bunx nuxt preview",
    "postinstall": "bunx nuxt prepare && bunx prisma generate --schema=prisma/schema.prisma",
    "lint": "bunx eslint .",
    "typecheck": "bunx nuxt typecheck",
    "test": "bunx vitest",
    "test:watch": "bunx vitest --watch",
    "test:coverage": "bunx vitest --coverage",
    "test:unit": "bunx vitest --project unit",
    "test:nuxt": "bunx vitest --project nuxt"
  },
